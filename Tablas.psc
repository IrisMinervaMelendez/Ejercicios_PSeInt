Algoritmo sin_titulo
	Repetir
		Escribir "Ingrese un número para ver la tabla de multiplicar"
		Leer num
			i<-1
			Mientras i >= 1 <= 33 Hacer
				i = 1+1 
				Concatenar(num" x",ConvertirATexto(i),Concatenar("="(ConvertirATexto(num*i))
		FinMientras
			
		Escribir "Desea que se muestre otra tabla?
		Escribir "1)si, 2)no
		Leer tablanueva
	Hasta Que tablanueva = 2
	Escribir "Fin de la operación"
	FinSegun

